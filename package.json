{
  "name": "@ofqual/portal-organisations",
  "version": "0.4.52",
  "main": "dist/module/index.js",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@ofqual/portal-activity-feed": "^0.1.10",
    "@ofqual/portal-auth": "^0.3.21",
    "@ofqual/portal-client-shell": "^0.1.41",
    "@ofqual/portal-components": "^0.1.49",
    "@ofqual/portal-core": "^0.1.46",
    "@ofqual/portal-organisations-scope": "^0.1.3",
    "@testing-library/jest-dom": "^5.11.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/sinon": "^9.0.8",
    "copyfiles": "^2.3.0",
    "date-fns": "^2.16.1",
    "deepmerge": "^4.2.2",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.8.3",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux-saga": "^1.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "use-debounce": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html",
    "eject": "react-scripts eject",
    "dist": "rimraf dist; tsc --noEmit false; copyfiles -u 1 \"./src/**/*.png\" \"./src/**/*.svg\" \"./src/**/*.scss\" dist",
    "serve:ci": "npx pm2 serve build 3000 --spa",
    "serve:kill": "npx pm2 kill",
    "testcafe:run": "testcafe testcafe/integration",
    "testcafe:run:report": "npm run testcafe:run -- --reporter html:reports/report.html",
    "testcafe:run:live": "npm run testcafe:run -- --live",
    "testcafe:ci": "testcafe chrome:headless testcafe/integration --reporter spec,xunit:report.xml -s path=./artifacts/screenshots,takeOnFails=true,fullPage=true --app \"npm run start\" --app-init-delay 120000",
    "testcafe:run/OrganisationList": "testcafe testcafe/integration/OrganisationList.ts",
    "testcafe:run/OrganisationViewDetails": "testcafe testcafe/integration/OrganisationViewDetails.ts",
    "CUCUMBER": "#############",
    "SERENITY REPORTING": "#############################################################",
    "serenity:install": "npx serenity-bdd update",
    "serenity:report": "npx serenity-bdd run --source ./cucumber/reports/serenity/json --destination ./cucumber/reports/serenity/html",
    "_______": "########################################################################",
    "WINDOWS": "################# BELOW ARE THE WINDOWS (WIN) COMMANDS #################",
    "-------": "########################################################################",
    "wait:ci": "wait-on tcp:3000",
    "cucumber:ci": "BROWSER=chrome BROWSER_FLAGS=:headless npm run cucumber",
    "cucumber:win": "cucumber-js.cmd -p cucumber",
    "cucumber:wip:win": "TAGS=@wip npm run cucumber:win",
    "cucumber:publish:win": "npm run cucumber:win -- --publish",
    "cucumber:parallel:win": "npm run cucumber:win -- --parallel 2 ; node ./cucumber/cucumber-html.config && node ./cucumber/cucumber-junit.config && rm -rf ./cucumber/reports/metadata.json",
    "cucumber": "TS_NODE_PROJECT=cucumber/tsconfig.cucumber.json npx cucumber-js -p cucumber --tags 'not @skip'",
    "cucumber:wip": "SCENARIO_RETRY=0 TAGS=@wip npm run cucumber",
    "cucumber:wip_windows": "set SCENARIO_RETRY=0&&set TAGS=@wip&&set TS_NODE_PROJECT=cucumber/tsconfig.cucumber.json&&npx cucumber-js -p cucumber",
    "cucumber:publish": "npm run cucumber -- --publish",
    "cucumber:parallel": "npm run cucumber -- --parallel 2 ; node ./cucumber/cucumber-html.config && node ./cucumber/cucumber-junit.config && rm -rf ./cucumber/reports/metadata.json",
    "######## LIVE MODE ########": "",
    "cucumber:live": "LIVE_MODE=on nodemon --config nodemon.json",
    "cucumber:live:wip": "TAGS=@wip npm run cucumber:live",
    "######## LINTING & FORMATING ########": "",
    "format": "npx prettier 'cucumber/**/*.{ts,json,scss,html}' --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@cucumber/cucumber": "~7.0.0",
    "@serenity-js/console-reporter": "^2.22.0",
    "@serenity-js/core": "^2.22.0",
    "@serenity-js/cucumber": "^2.22.0",
    "@serenity-js/serenity-bdd": "^2.22.0",
    "@types/chai": "~4.2.14",
    "@types/chai-string": "~1.4.2",
    "@types/mssql": "^6.0.7",
    "@types/node-fetch": "~2.5.8",
    "babel-jest": "^26.6.3",
    "base64-img": "~1.0.4",
    "chai": "~4.2.0",
    "chai-string": "~1.5.0",
    "cucumber-html-reporter": "~5.3.0",
    "cucumber-junit": "~1.7.1",
    "cucumber-junit-convert": "~1.1.0",
    "jest-junit": "^12.0.0",
    "mssql": "^6.3.1",
    "node-fetch": "~2.6.1",
    "nodemon": "~2.0.7",
    "pm2": "^4.5.4",
    "prettier": "~2.2.1",
    "screenshot-desktop": "~1.12.3",
    "serve": "^11.3.2",
    "testcafe": "^1.10.1",
    "testcafe-react-selectors": "^4.1.1",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-xunit": "^2.1.0",
    "wait-on": "^5.2.1",
    "winston": "~3.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "verbose": true,
    "testURL": "http://localhost:8080/",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!\\@ofqual/portal-core/).+"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
